<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:focusable="false"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:rotationX="0"
    android:rotationY="0"
    tools:context="net.magnetspace.frequency.GUI.FrequencyPlayerActivity">

    <Button
        android:id="@+id/startPlayingBtn"
        style="@android:style/ButtonBar"
        android:layout_width="329dp"
        android:layout_height="67dp"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="-88dp"
        android:layout_marginBottom="0dp"
        android:layout_toLeftOf="@id/power4"
        android:layout_toEndOf="@+id/power3"
        android:layout_toRightOf="@id/power2"
        android:background="@drawable/custom_button"
        android:fontFamily="monospace"
        android:onClick="OnStartPlayingBtn"
        android:text="@string/start"
        android:textColor="@android:color/background_light"
        android:textSize="30dp"
        android:textStyle="normal"
        tools:ignore="RtlHardcoded,SpUsage" />

    <SeekBar
        android:id="@+id/seekBar"
        style="@style/CustomSeekBar"
        android:layout_width="909dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView2"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="26dp"
        android:layout_marginLeft="26dp"
        android:layout_marginEnd="25dp"
        android:layout_marginRight="25dp"
        android:layout_marginBottom="40dp"
        android:indeterminateOnly="false"
        android:progress="5"
        android:visibility="visible" />

    <Button
        android:id="@+id/button6"
        style="@android:style/ButtonBar"
        android:layout_width="40dp"
        android:layout_height="67dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_toStartOf="@+id/power3"
        android:layout_toLeftOf="@+id/power3"
        android:background="@drawable/custom_button"
        android:fontFamily="monospace"
        android:onClick="onBackBtn2"
        android:text="@string/back"
        android:textColor="@android:color/background_light"
        android:textSize="30dp"
        android:textStyle="normal"
        tools:background="@drawable/custom_button"
        tools:ignore="SpUsage"
        tools:text="@string/back" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_above="@id/textView2"
        android:layout_alignBaseline="@id/myvisualizerview"
        android:layout_alignBottom="@id/button6"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="false"
        android:layout_marginStart="610dp"
        android:layout_marginLeft="610dp"
        android:layout_marginEnd="2dp"
        android:layout_marginRight="2dp"
        android:layout_marginBottom="77dp"
        android:layout_toRightOf="@id/power1"
        android:fontFamily="monospace"
        android:text="@string/_0_00_0_00"
        android:textAlignment="center"
        android:textSize="20dp"
        tools:ignore="RtlHardcoded,SpUsage" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/startPlayingBtn"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="10dp"
        android:fontFamily="monospace"
        android:text="New Text"
        android:textSize="20dp"
        tools:ignore="SpUsage" />

    <net.magnetspace.frequency.GUI.CustomView.VisualizerView
        android:id="@+id/myvisualizerview"
        android:layout_width="507dp"
        android:layout_height="251dp"
        android:layout_above="@id/power1"
        android:layout_alignParentTop="true"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="5dp"
        android:layout_toStartOf="@+id/power4"
        android:layout_toLeftOf="@+id/power4" />

    <Button
        android:id="@+id/power1"
        style="@android:style/ButtonBar"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:layout_marginBottom="10dp"
        android:background="@drawable/custom_thumb_state_default"
        android:fontFamily="monospace"
        android:onClick="setPower1"
        android:paddingTop="10dip"
        android:text="1"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textAppearance="@android:style/TextAppearance.DeviceDefault"
        android:textColor="@android:color/background_light"
        android:textColorHighlight="@android:color/background_light"
        android:textColorHint="@android:color/background_light"
        android:textColorLink="@android:color/background_light"
        android:textCursorDrawable="@drawable/custom_button"
        android:textSize="35sp"
        android:textStyle="normal|bold"
        android:typeface="normal"
        android:visibility="invisible"
        tools:ignore="HardcodedText,SmallSp" />

    <Button
        android:id="@+id/power2"
        style="@android:style/ButtonBar"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_alignBottom="@id/power1"
        android:layout_marginStart="70dp"
        android:layout_marginLeft="70dp"
        android:layout_marginBottom="0dp"
        android:layout_toEndOf="@+id/power1"
        android:layout_toRightOf="@id/power1"
        android:background="@drawable/custom_thumb_state_default"
        android:fontFamily="monospace"
        android:onClick="setPower2"
        android:text="2"
        android:textColor="@android:color/background_light"
        android:textSize="35dp"
        android:textStyle="normal|bold"
        android:visibility="invisible"
        tools:ignore="HardcodedText,SmallSp" />

    <Button
        android:id="@+id/power3"
        style="@android:style/ButtonBar"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignBottom="@+id/power2"
        android:layout_marginStart="2dp"
        android:layout_marginLeft="2dp"
        android:layout_marginBottom="0dp"
        android:layout_toEndOf="@+id/power2"
        android:layout_toRightOf="@+id/power2"
        android:background="@drawable/custom_thumb_state_default"
        android:fontFamily="monospace"
        android:onClick="setPower3"
        android:text="1"
        android:textColor="@android:color/background_light"
        android:textSize="40sp"
        android:textStyle="normal|bold"
        tools:ignore="HardcodedText,SmallSp" />

    <Button
        android:id="@+id/power4"
        style="@android:style/ButtonBar"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignBottom="@+id/power3"
        android:layout_marginStart="88dp"
        android:layout_marginLeft="88dp"
        android:layout_marginBottom="0dp"
        android:layout_toEndOf="@+id/power3"
        android:layout_toRightOf="@+id/power3"
        android:background="@drawable/custom_thumb_state_default"
        android:fontFamily="monospace"
        android:onClick="setPower4"
        android:text="2"
        android:textColor="@android:color/background_light"
        android:textSize="40sp"
        android:textStyle="bold"
        android:visibility="visible"
        tools:ignore="HardcodedText,SmallSp" />

    <Button
        android:id="@+id/power5"
        style="@android:style/ButtonBar"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignBottom="@+id/power4"
        android:layout_marginStart="88dp"
        android:layout_marginLeft="88dp"
        android:layout_marginBottom="0dp"
        android:layout_toEndOf="@+id/power4"
        android:layout_toRightOf="@+id/power4"
        android:background="@drawable/custom_thumb_state_default"
        android:clickable="true"
        android:cursorVisible="true"
        android:fontFamily="monospace"
        android:onClick="setPower5"
        android:text="3"
        android:textColor="@android:color/background_light"
        android:textSize="40sp"
        android:textStyle="bold"
        android:visibility="visible"
        tools:ignore="HardcodedText,SmallSp"
        tools:visibility="visible" />

    <ListView
        android:id="@+id/listView3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignBottom="@id/divider"
        android:layout_alignParentTop="true"
        android:layout_marginStart="80dp"
        android:layout_marginLeft="80dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="1dp"
        android:layout_toEndOf="@+id/myvisualizerview"
        android:listSelector="#e0e1d6"
        android:smoothScrollbar="false"
        android:textFilterEnabled="false" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="236dp"
        android:layout_height="54dp"
        android:layout_below="@+id/space"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="-171dp"
        android:layout_marginLeft="-171dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="256dp"
        android:layout_marginRight="256dp"
        android:layout_marginBottom="17dp"
        android:layout_toEndOf="@+id/power5"
        android:layout_toRightOf="@+id/power5"
        android:fontFamily="monospace"
        android:text="New Text"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textSize="15dp"
        android:visibility="invisible"
        tools:ignore="RtlHardcoded,UnknownId"
        tools:visibility="invisible" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="230dp"
        android:layout_height="64dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="200dp"
        android:layout_marginLeft="200dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginBottom="0dp"
        android:layout_toRightOf="@id/startPlayingBtn"
        android:text="www.msp-shop.hu"
        android:textSize="25dp"
        android:visibility="visible"
        tools:ignore="HardcodedText"
        tools:text="www.msp-shop.hu"
        tools:visibility="visible" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="95dp"
        android:layout_height="95dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="110dp"
        android:layout_marginLeft="110dp"
        android:layout_marginTop="45dp"
        android:src="@mipmap/cardiology" />

    <View
        android:id="@+id/divider"
        android:layout_width="2dp"
        android:layout_height="300dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="350dp"
        android:layout_marginLeft="350dp"
        android:layout_marginBottom="350dp"
        android:background="@android:drawable/status_bar_item_background" />


</RelativeLayout>
